create table strukturv (
ID number(5,0) not null constraint strukturv_pk primary key, 
nosauk varchar2(50) not null unique);

CREATE SEQUENCE seq_strukt_id
MINVALUE 1
START WITH 1
INCREMENT BY 1
CACHE 10;

